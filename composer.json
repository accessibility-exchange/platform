{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "version": "1.0.0-alpha2",
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "blade-ui-kit/blade-heroicons": "^1.3",
        "blade-ui-kit/blade-icons": "^1.2",
        "codeat3/blade-forkawesome": "^1.8",
        "codezero/laravel-unique-translation": "^3.5",
        "doctrine/dbal": "^3.3",
        "elhebert/laravel-sri": "^3.1",
        "fluid-project/hearth": "dev-main",
        "fruitcake/laravel-cors": "^3.0",
        "graham-campbell/markdown": "^14.0",
        "guzzlehttp/guzzle": "^7.4",
        "laravel/framework": "^9.10",
        "laravel/tinker": "^2.7",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^2.10",
        "makeabledk/laravel-eloquent-status": "^2.0",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-ciphersweet": "^1.0",
        "spatie/laravel-collection-macros": "^7.11",
        "spatie/laravel-db-snapshots": "^2.2",
        "spatie/laravel-login-link": "^1.1",
        "spatie/laravel-medialibrary": "^10.3",
        "spatie/laravel-options": "^1.0",
        "spatie/laravel-schemaless-attributes": "^2.3",
        "spatie/laravel-sql-commenter": "^1.1",
        "spatie/laravel-translatable": "^6.0",
        "spatie/valuestore": "^1.3",
        "symfony/http-foundation": "~6.1.2",
        "theiconic/name-parser": "^1.2",
        "umpirsky/language-list": "^1.1"
    },
    "require-dev": {
        "amirami/localizator": "^0.9.1@alpha",
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.12",
        "fakerphp/faker": "^1.19",
        "laravel/dusk": "^6.22",
        "laravel/pint": "^0.2.3",
        "laravel/sail": "^1.13",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^2.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-faker": "^1.0",
        "pestphp/pest-plugin-laravel": "^1.2",
        "pestphp/pest-plugin-livewire": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ignition": "^1.1",
        "spatie/laravel-ray": "^1.29",
        "worksome/request-factories": "^1.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "analyze": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint",
        "test": "php artisan test",
        "test-coverage": "php artisan test --coverage"
    }
}
