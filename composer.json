{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "blade-ui-kit/blade-heroicons": "^0.3.1",
        "blade-ui-kit/blade-icons": "^0.5.1",
        "chinleung/laravel-locales": "^1.2",
        "chinleung/laravel-multilingual-routes": "^2.7",
        "elhebert/laravel-sri": "^3.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/fortify": "^1.7",
        "laravel/framework": "^8.12",
        "laravel/tinker": "^2.5",
        "spatie/laravel-backup": "^7.2",
        "spatie/laravel-flash": "^1.8",
        "spatie/laravel-permission": "^4.0",
        "spatie/laravel-sluggable": "^2.6",
        "spatie/laravel-translatable": "^4.6"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/dusk": "^6.13",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "nunomaduro/larastan": "^0.7.0",
        "phpunit/phpunit": "^9.3.3",
        "psalm/plugin-laravel": "^1.4",
        "squizlabs/php_codesniffer": "^3.5",
        "vimeo/psalm": "^4.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "analyze": [
            "vendor/bin/phpstan analyse"
        ],
        "lint": [
            "vendor/bin/phpcs"
        ]
    }
}
