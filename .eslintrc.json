{
    "extends": [
        "fluid",
        "prettier"
    ],
    "ignorePatterns": ["public/**/*.js"],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "strict": ["error", "never"]
    }
}
